{
  "id": "euDirectives",
  "endpoint": {
    "url": "https://ldf.fi/finestlaw/sparql",
    "useAuth": true,
    "prefixesFile": "SparqlQueriesPrefixes.js"
  },
  "sparqlQueriesFile": "SparqlQueriesStatutes.js",
  "baseURI": "http://data.europa.eu/eli",
  "URITemplate": "<BASE_URI>/<LOCAL_ID>",
  "facetClass": "lss:EULegalAct",
  "frontPageImage": "main_page/euDirectives.png",
  "searchMode": "faceted-search",
  "defaultActiveFacets": ["prefLabel"],
  "defaultTab": "table",
  "defaultInstancePageTab": "content",
  "resultClasses": {
    "euDirectives": {
      "paginatedResultsConfig": {
        "tabID": 0,
        "component": "ResultTable",
        "tabPath": "table",
        "tabIcon": "CalendarViewDay",
        "propertiesQueryBlock": "euDirectiveProperties",
        "pagesize": 20,
        "sortBy": null,
        "sortDirection": null
      },
      "instanceConfig": {
        "propertiesQueryBlock": "euDirectivePropertiesInstancePage",
        "instancePageResultClasses": {
          "instancePageContextualContent": {
            "tabID": 0,
            "component": "InstancePageContextualContent",
            "tabPath": "content",
            "tabIcon": "Subject"
          },
          "instancePageTable": {
            "tabID": 1,
            "component": "InstancePageTable",
            "tabPath": "table",
            "tabIcon": "CalendarViewDay"
          },
          "instancePageExport": {
            "tabID": 2,
            "component": "Export",
            "tabPath": "export",
            "tabIcon": "CloudDownload",
            "pageType": "instancePage"
          }
        }
      }
    },
    "statutesByYear": {
      "tabID": 1,
      "component": "ApexCharts",
      "createChartData": "createMultipleLineChartData",
      "stroke": {
        "curve": "straight",
        "width": 2
      },
      "xaxisTickAmount": 20,
      "title": "",
      "xaxisTitle": "",
      "yaxisTitle": "",
      "tabPath": "by_year",
      "tabIcon": "ShowChart",
      "sparqlQuery": "statutesByYearQuery",
      "facetClass": "statutes",
      "filterTarget": "statute",
      "resultMapper": "mapMultipleLineChart",
      "resultMapperConfig": {
        "fillEmptyValues": true
      }
    },
    "export": {
      "tabID": 2,
      "component": "Export",
      "tabPath": "export",
      "tabIcon": "CloudDownload",
      "resultClass": "perspective1",
      "facetClass": "perspective1"
    },
    "knowledgeGraphMetadata": {
      "sparqlQuery": "knowledgeGraphMetadataQuery",
      "resultMapper": "makeObjectList"
    }
  },
  "properties": [
    {
      "id": "source",
      "valueType": "object",
      "makeLink": true,
      "externalLink": true,
      "sortValues": true,
      "numberedList": false,
      "onlyOnInstancePage": true
    },
    {
      "id": "prefLabel",
      "valueType": "object",
      "makeLink": true,
      "externalLink": false,
      "sortValues": true,
      "numberedList": false,
      "minWidth": 250,
      "enableCountryFlag": true
    }
  ],
  "facets": {
    "prefLabel": {
      "sortByPredicate": "skos:prefLabel"
    },
    "textSearch": {
      "containerClass": "one",
      "facetType": "text",
      "filterType": "textFilter"
    }
  }
}
